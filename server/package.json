{
  "name": "infinity-catalogue-server",
  "version": "0.1.0",
  "scripts": {
    "compile": "tsc",
    "start": "npm run compile && node ./dist/index.js",
    "dev": "nodemon - exec tsx ./src/index.ts",
    "sync-models": "npm run compile && node ./dist/sync-models.js",
    "drop-tables": "npm run compile && node ./dist/drop-tables.js",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "axios": "^1.6.7",
    "bluebird": "^3.7.2",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "jwt-decode": "^4.0.0",
    "pg": "^8.11.3",
    "sequelize": "^6.36.0",
    "slug": "^8.2.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@types/bluebird": "^3.5.42",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/slug": "^5.0.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "3.2.4",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "npm": ">=10.2.4",
    "node": ">=20.11.0"
  }
}
